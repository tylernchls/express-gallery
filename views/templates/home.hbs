<h1>Project Gallery</h1>

{{#each project}}
<a href="/gallery/{{this.id}}">
  <div class="projects">
    <p>link: {{this.link}}</p>
    <div class="desc">
      <p>description: {{this.description}}</p>
    </div>
  </div>
</a>
{{/each}}

<div class="np">
  <input type=button id="np_button" onClick="location.href='/gallery/new'" value='New Project'>
</div>

<div class="lg_out">
  <a href="/logout"><button id="logout_btn">Logout</button></a>
</div>


