{{#each project}}
<a href="/gallery/{{this.id}}">
  <div class="projects">
    <p>Link: {{this.link}}</p>
    <div class="desc">
      <p>Description: {{this.description}}</p>
    </div>
  </div>
</a>
{{/each}}

<div>
  <input type=button id="np_btn" onClick="location.href='/gallery/new'" value='New Project'>
</div>

<div>
  <input type=button id="reg_btn" onClick="location.href='/gallery/register'" value='Register'>
</div>

<div>
  <a href="/logout"><button id="logout_btn">Logout</button></a>
</div>


